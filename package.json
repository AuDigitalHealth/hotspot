{
  "name": "hotspot",
  "description": "A human-friendly HTML landing page for users resolving URLs within their browser that point to resources on a FHIR server.",
  "license": "Apache-2.0",
  "author": {
    "name": "John Grimes",
    "email": "John.Grimes@digitalhealth.gov.au"
  },
  "repository": "github:AuDigitalHealth/hotspot",
  "dependencies": {
    "axios": "^0.18.1",
    "babel-runtime": "6.26.0",
    "html-to-react": "^1.3.3",
    "js-yaml": "~3.7.0",
    "lodash.intersection": "^4.4.0",
    "lodash.omitby": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-highlight": "briancappello/react-highlight#react-v16-compiled",
    "react-router-dom": "^4.1.1",
    "sanitize-html": "^1.14.1",
    "svgo": "^0.7.0",
    "vkbeautify": "^0.99.2",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "dockerize": "bash scripts/dockerize.sh",
    "push-docker": "bash scripts/pushDockerImage.sh",
    "test": "node scripts/test.js --env=jsdom --verbose",
    "test-ci": "CI=true node scripts/test.js --env=jsdom",
    "test-coverage": "CI=true node scripts/test.js --env=jsdom --coverage",
    "lint": "eslint --max-warnings 0 src",
    "format-staged": "bash scripts/formatStagedFiles.sh"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "codecov": "^3.6.5",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.0.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.18.2",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-react-app": "^1.0.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-to-react": "^1.3.3",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "pre-commit": "^1.2.2",
    "prettier": "^1.10.2",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-dev-utils": "^5.0.0",
    "react-test-renderer": "^16.2.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "xmldom": "^0.1.27"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "pre-commit": [
    "lint",
    "format-staged",
    "test-ci"
  ],
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
