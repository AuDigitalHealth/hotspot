{
  "parser": "babel-eslint",
  "plugins": ["react", "jest", "jsx-a11y"],
  "env": {
    "builtin": true,
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "Generator": false,
    "onmessage": true
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "extends": ["react-app"],
  "rules": {
    "no-console": "warn",
    "guard-for-in": "off",
    "no-var": "error"
  }
}
