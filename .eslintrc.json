{
  "parser": "babel-eslint",
  "plugins": ["react", "jest"],
  "env": {
    "builtin": true,
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "Generator": false,
    "onmessage": true
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jest/recommended"
  ],
  "rules": {
    "no-console": ["warn"]
  }
}
